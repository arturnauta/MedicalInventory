
<div class="contanier">
      <%= form_for @products_by_lab do |pl| %>
        <div class="form-group">
          <%= pl.label :laboratory %>
          <select name="product[laboratory_id]" class="selectpicker">
            <%= @laboratories.each do |l|  %>
                <option value="<%= l.id %>"> <%= l.name %> </option>
            <% end  %>
          </select>
      </div>
      <% end %>
</div>
